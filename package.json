{
    "name": "example",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "prepare": "husky install",
        "prisma:seed": "cd ./src && npx prisma db seed",
        "prisma:generate": "cd ./src && npx prisma generate",
        "prisma:migrate": "cd ./src && npx prisma migrate dev",
        "prisma:format": "cd ./src && npx prisma format"
    },
    "dependencies": {
        "@faker-js/faker": "^7.6.0",
        "@golevelup/nestjs-rabbitmq": "^3.5.0",
        "@nestjs/axios": "^2.0.0",
        "@nestjs/common": "^9.3.9",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.3.9",
        "@nestjs/cqrs": "^9.0.3",
        "@nestjs/mongoose": "^9.2.1",
        "@nestjs/passport": "^9.0.3",
        "@nestjs/platform-express": "^9.3.9",
        "@nestjs/swagger": "^6.2.1",
        "@nestjs/terminus": "^9.2.1",
        "@prisma/client": "^4.10.1",
        "@types/express-handlebars": "^5.3.1",
        "@types/luxon": "^3.2.0",
        "aws-sdk": "^2.1321.0",
        "bson-objectid": "^2.0.4",
        "cache-manager": "^5.1.7",
        "cache-manager-redis-store": "^2.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "clone-deep": "^4.0.1",
        "dot-object": "2.1.4",
        "express-handlebars": "^6.0.7",
        "hbs": "^4.2.0",
        "ioredis": "^5.3.1",
        "jwks-rsa": "^3.0.1",
        "luxon": "^3.2.1",
        "mongodb-query-parser": "2.4.11",
        "mongoose": "6.10.0",
        "nestjs-command": "^3.1.3",
        "nestjs-i18n": "^10.2.6",
        "node-fetch": "^2.6.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "prismix": "^1.0.21",
        "realm": "11.5.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^4.1.2",
        "rxjs": "^7.8.0",
        "swagger-ui-express": "^4.6.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.2.0",
        "@nestjs/schematics": "^9.0.4",
        "@nestjs/testing": "^9.3.9",
        "@types/cache-manager": "^4.0.2",
        "@types/express": "^4.17.17",
        "@types/jest": "29.4.0",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.14.1",
        "@types/node-fetch": "^2.6.1",
        "@types/supertest": "^2.0.12",
        "@types/yargs": "^17.0.22",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "jest": "^29.4.3",
        "prettier": "^2.8.4",
        "prisma": "^4.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.5",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "typescript": "4.9.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node",
        "moduleNameMapper": {
            "^@app/(.*)$": "<rootDir>/../src/$1"
        }
    },
    "prisma": {
        "seed": "ts-node src/prisma/seed.ts"
    }
}
